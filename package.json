    "husky:pre-push": "npm run lint ",
    "start": " react-scripts start",
    "build": "react-scripts build",
    "test": "jest --coverage --coverageReporters=text --coverageReporters=text-summary ",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [],
    "coverageThreshold": {
      "global": {
          "branches": 40,
          "functions":40,
          "lines": 40,
          "statements": 40
      }
  }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js, jsx}": [
      "npm run lint:fix",
      "git add"
    ],
    "src/**/*.{js,jsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {}
